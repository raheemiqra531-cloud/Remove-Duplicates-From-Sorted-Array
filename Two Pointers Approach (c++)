int removeDuplicates(vector<int>& nums) {
    if (nums.size() == 0) return 0;

    int i = 0;  // points to last unique element

    for (int j = 1; j < nums.size(); j++) {
        if (nums[j] != nums[i]) {
            i++;
            nums[i] = nums[j];
        }
    }

    return i + 1; // number of unique elements
}
